    "docker:dev": "ts-node-dev --poll --respawn --transpile-only src/server.ts"
  },
  "dependencies": {
    "cors": "^2.8.5",
    "dns-packet": "^5.6.1",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "ip": "^1.1.8",
    "node-fetch": "^2.7.0"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/dns-packet": "^5.2.7",
    "@types/express": "^4.17.21",
    "@types/ip": "^1.1.3",
    "@types/node": "^16.18.68",
    "@types/node-fetch": "^2.6.9",
    "@typescript-eslint/eslint-plugin": "^5.62.0",
    "@typescript-eslint/parser": "^5.62.0",
    "eslint": "^8.55.0",
    "rimraf": "^5.0.5",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^4.9.5"
  }
