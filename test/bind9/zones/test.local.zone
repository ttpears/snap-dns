$ORIGIN .
$TTL 3600	; 1 hour
test.local		IN SOA	ns1.test.local. admin.test.local. (
				2025011504 ; serial
				3600       ; refresh (1 hour)
				1800       ; retry (30 minutes)
				604800     ; expire (1 week)
				86400      ; minimum (1 day)
				)
			NS	ns1.test.local.
			NS	ns2.test.local.
			MX	10 mail.test.local.
			MX	20 backup-mail.test.local.
			TXT	"testing123"
			TXT	"v=spf1 mx a ~all"
$ORIGIN test.local.
_dmarc			TXT	"v=DMARC1; p=quarantine; rua=mailto:dmarc@test.local"
_dmarc2			TXT	"\"v=DMARC1; p=quarantine; rua=mailto:dmarc@test.local\""
